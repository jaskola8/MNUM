function func = chebyshev(x, k)
    if k == 0
        func = 1;
        return
    end
    if k == 1
        func = x;
        return
    end
    v = [1, x, (2*x*x)-1];
    for i = 2:k+1
        tmp = v(:,3);
        disp(tmp);
        v(:,3) = 2*x*(v(:,3))-v(:,2);
        disp(v(:,3));
        v(:,2) = tmp;
    end
    func = v(:,3);