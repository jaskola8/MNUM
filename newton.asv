function func = newton(data, x)
    func = 0.0;
    u = data(1,:);
    %a = size(data(1,:));
    r = zeros(4);
    r(:,1) = data(1,:);
    disp("u = "+ data)
    disp(u);
    disp(r);
    disp("r == " + r(2,1));
    for j = 1:1:3
        z = 4;
        for i = 5-j :-j:2
            q = r(i,j)-r(i-1,j-1);
            w = u(z)- u(z-j);
            r(i-1,j) = q/w;
            z = z-1;
        end
    end
    disp(r)
            
    