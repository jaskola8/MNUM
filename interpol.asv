function xs = interpol(func, x1, xn, N)
data = zeros(
data(1,:) = linspace(x1, xn, N);

for i = 1:N
    data(2,:) = func(data(i));
end
xs = zeros(3, 1000);
xs(1,:) = linspace(x1, xn, 1000);
for i = 1:1000
    xs(2,i) = func(xs(1,i));
    xs(3,1) = legrange(data, xs(1,i));
end

plot(xs(1,:),xs(2,:),xs(1,:),xs(3,:),"r--")


    
    